@model IEnumerable<Tenders.Models.OrgTender>



</br>
<div class="form-group">

    <a class="btn btn-default"  href=" @Url.Action("Create", "OrgTenders")">Создать нового организатора</a>
  
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ListOrgTender)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ListOrgTender)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.OrgTenderID }) |       
            @Html.ActionLink("Удалить", "Delete", new { id=item.OrgTenderID })
        </td>
    </tr>
}

</table>
