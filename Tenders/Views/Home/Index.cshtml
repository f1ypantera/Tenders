@model IEnumerable<Tenders.Models.Tender>





<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />

</head>
<body>

    <h1>Тендеры</h1>


    @foreach (var item in @Model)
    {

        <div class="well">
            <strong> Тендер номер @item.TenderID</strong>
            <h3>
                <span class="lead">@item.SubjectTender</span>
            </h3>
            
            <p>@item.OrgTender.ListOrgTender</p>
            <span class="pull-right label label-primary">@item.Budget</span>
            <p>@item.CurrencyBudget.ListCurrencyBudget</p>
            <p>@item.SponsorShip</p>



        </div>

    }
       
           

   

      



</body>
</html>