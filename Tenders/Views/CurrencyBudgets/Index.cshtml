@model IEnumerable<Tenders.Models.CurrencyBudget>




</br>
<div class="form-group">

    <a class="btn btn-default" href=" @Url.Action("Create", "CurrencyBudgets")">Создать новый бюджет</a>

</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ListCurrencyBudget)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ListCurrencyBudget)
            </td>
            <td>
                @Html.ActionLink("Редактировать", "Edit", new { id = item.CurrencyBudgetID }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.CurrencyBudgetID })
            </td>
        </tr>
    }

</table>
